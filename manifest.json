{
  "manifest_version": 3,
  "name": "KidsDiary Photo Downloader",
  "version": "0.0.3",
  "description": "A Chrome extention that allows users to download photos from KidsDiary with a single click.",
  "author": "1nm",
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "action": {},
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://kidsdiary.jp/*"
  ]
}